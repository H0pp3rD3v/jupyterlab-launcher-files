    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/H0pp3rD3v/jupyterlab-launcher-files",
  "bugs": {
    "url": "https://github.com/H0pp3rD3v/jupyterlab-launcher-files/issues"
  },
  "license": "BSD-3-Clause",
  "author": "H0pp3rD3v",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
